{
  "name": "barbertime-api",
  "version": "1.0.0",
  "description": "API do sistema de agendamento - BarberTime",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate:deploy": "prisma generate && prisma db push --accept-data-loss",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:reset": "npx prisma migrate reset",
    "seed": "node prisma/seed.js",
    "db:setup": "npx prisma migrate dev && node prisma/seed.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": ["barbearia", "agendamento", "api", "rest"],
  "author": "Guilhermy Deveza - Fundação & Arquitetura",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "prisma": "^5.22.0",
    "swagger-ui-express": "^5.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.0.2"
  }
}
